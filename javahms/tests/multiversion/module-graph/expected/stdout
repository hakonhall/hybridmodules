digraph "module graph" {
  subgraph cluster_hybrid {
    graph [ style=dotted; label=<<font face="Helvetica">HYBRID MODULES</font>>; ]
    node [ color=blue; ]
    "intermediate@"
    "m@1"
    "m@2"
    "main@" [ style=bold; label=<<b><u>main@</u></b>>; ]
  }
  subgraph cluster_platform {
    graph [ style=dotted; label=<<font face="Helvetica">PLATFORM MODULES</font>>; ]
    node [ color=red; ]
    "java.base"
  }
  "intermediate@" -> "m@2"
  "intermediate@" -> "java.base"
  "m@1" -> "java.base"
  "m@2" -> "java.base"
  "main@" -> "m@1"
  "main@" -> "intermediate@"
  "main@" -> "java.base"
}

